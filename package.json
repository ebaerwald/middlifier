{
  "name": "middlifier",
  "version": "1.0.15",
  "description": "Middlifier provides a command where you can give instructions to build a general backend and middleware template.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "node ./dist/index.js",
    "start-build": "npm run build && node ./dist/index.js",
    "w-delete": "rmdir /s /q dist gen",
    "l-delete": "rm -rf dist gen",
    "build": "tsc && node ./src/build.js",
    "generate": "node ./dist/build.js",
    "generate-build": "npm run build && node ./dist/build.js"
  },
  "keywords": [],
  "author": "ebaerwald",
  "license": "ISC",
  "bin": {
    "middlifier-init": "./dist/init.js",
    "middlifier-build": "./dist/build.js"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/express": "^4.17.21",
    "@types/prompt-sync": "^4.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "fs": "^0.0.1-security",
    "middlifier": "^1.0.15",
    "mongoose": "^8.0.2",
    "nodemon": "^3.0.2",
    "prompt-sync": "^4.2.0"
  }
}
